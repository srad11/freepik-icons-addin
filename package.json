    "sideload:production": "bash scripts/install.sh --production",
    "dev-certs": "office-addin-dev-certs install"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "office-addin-dev-certs": "^2.0.6",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "playwright": "^1.58.2"
  }
}
