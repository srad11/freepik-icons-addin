{
  "name": "freepik-icons-powerpoint-addin",
  "version": "1.0.0",
  "description": "FreePik Icons - PowerPoint Add-in for searching, generating, and inserting icons from Freepik",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
